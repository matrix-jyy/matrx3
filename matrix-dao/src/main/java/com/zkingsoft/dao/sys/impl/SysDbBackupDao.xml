	</resultMap>
	
	
	
	<!--  插入方法   -->
	<insert id="insert" parameterType="SysDbBackup"
		useGeneratedKeys="true" keyProperty="buId">
		INSERT INTO sys_db_backup (
			bu_id,
			bu_db_ip,
			bu_db_port,
			bu_db_name,
			bu_db_uname,
			bu_db_pwd,
			bu_status,
			bu_size,
			bu_path,
			bu_err_msg
		)
	VALUES (
			#{buId},
			#{buDbIp},
			#{buDbPort},
			#{buDbName},
			#{buDbUname},
			#{buDbPwd},
			#{buStatus},
			#{buSize},
			#{buPath},
			#{buErrMsg}
	)
	</insert>
	
	
	
	<!--  批量插入   -->
	<insert id="batchInsert" parameterType="java.util.List">
		INSERT INTO sys_db_backup (
			bu_id,
			bu_db_ip,
			bu_db_port,
			bu_db_name,
			bu_db_uname,
			bu_db_pwd,
			bu_status,
			bu_size,
			bu_path,
			bu_err_msg
		)
	VALUES 
	<foreach collection="list" item="item" index="index" separator=",">(
			#{item.buId},
			#{item.buDbIp},
			#{item.buDbPort},
			#{item.buDbName},
			#{item.buDbUname},
			#{item.buDbPwd},
			#{item.buStatus},
			#{item.buSize},
			#{item.buPath},
			#{item.buErrMsg}
	)</foreach>
	</insert>
	
	
	
	
	
	<!--  根据Map更新 部分更新   -->
	<update id="updateByMap" parameterType="java.util.HashMap" >
		UPDATE sys_db_backup
		<set>
			<if test="_parameter.containsKey('buDbIp')">
					bu_db_ip = #{buDbIp},
				</if>		
			<if test="_parameter.containsKey('buDbPort')">
					bu_db_port = #{buDbPort},
				</if>		
			<if test="_parameter.containsKey('buDbName')">
					bu_db_name = #{buDbName},
				</if>		
			<if test="_parameter.containsKey('buDbUname')">
					bu_db_uname = #{buDbUname},
				</if>		
			<if test="_parameter.containsKey('buDbPwd')">
					bu_db_pwd = #{buDbPwd},
				</if>		
			<if test="_parameter.containsKey('buStatus')">
					bu_status = #{buStatus},
				</if>		
			<if test="_parameter.containsKey('buSize')">
					bu_size = #{buSize},
				</if>		
			<if test="_parameter.containsKey('buPath')">
					bu_path = #{buPath},
				</if>		
			<if test="_parameter.containsKey('buErrMsg')">
					bu_err_msg = #{buErrMsg},
				</if>		
		</set>
		WHERE bu_id=#{buId} 
	</update>
	
	
	
